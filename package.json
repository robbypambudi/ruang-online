    "@tanstack/react-table": "^8.7.9",
    "auto-zustand-selectors-hook": "^2.0.0",
    "axios": "1.2.1",
    "clsx": "^1.2.1",
    "immer": "^9.0.19",
    "lodash.get": "^4.4.2",
    "lucide-react": "^0.121.0",
    "next": "^13.1.6",
    "nprogress": "^0.2.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.10.0",
